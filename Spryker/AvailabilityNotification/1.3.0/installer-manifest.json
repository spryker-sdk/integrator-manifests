{
    "configure-env": [
        {
            "target": "\\Spryker\\Shared\\AvailabilityNotification\\AvailabilityNotificationConstants::BASE_URL_YVES_PORT",
            "value": "$yvesPort"
        },
        {
            "target": "\\Spryker\\Shared\\AvailabilityNotification\\AvailabilityNotificationConstants::STORE_TO_YVES_HOST_MAPPING",
            "value": {
                "DE": "getenv('SPRYKER_YVES_HOST_DE')",
                "AT": "getenv('SPRYKER_YVES_HOST_AT')",
                "US": "getenv('SPRYKER_YVES_HOST_US')"
            }
        }
    ]
}
